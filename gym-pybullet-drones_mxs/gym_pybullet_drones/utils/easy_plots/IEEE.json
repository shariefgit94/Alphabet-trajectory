{
  "info": {
    "name": "scientific",
    "description": "Space-efficient and color-blind friendly theme for printing on paper"
  },
  "params": {
    "lines": {
      "width": 2.5
    },
    "axes": {
      "bottom": true,
      "top": true,
      "left": true,
      "right": true,
      "xmargin": 0,
      "ymargin": 0,
      "zmargin": 0,
      "width": 2
    },
    "axis_labels": {
      "pad": -1,
      "size": "large"
    },
    "ticks": {
      "direction": "in",
      "draw_minor": true,
      "pad_major": 10,
      "width_major": 2,
      "width_minor": 1,
      "size_major": 10,
      "size_minor": 6
    },
    "tick_labels": {
      "size": "large"
    },
    "title": {
      "location": "center",
      "pad": 20,
      "size": "x-large"
    },
    "grid": {
      "draw": true,
      "axis": "both",
      "ticks": "major",
      "alpha": 0.5,
      "width": 1,
      "style": "--"
    },
    "fonts": {
      "family": "serif",
      "size": 20
    },
    "colors": {
      "figure_background_color": "white",
      "plot_background_color": "white",
      "text_color": "black",
      "line_color": "#333",
      "axes_color": "#555",
      "grid_color": "#555",
      "tick_color": "#555",
      "tick_label_color": "black",
      "palette": [
        "#56B4DF",
        "#D55E00",
        "#009E73",
        "#4EB67F",
        "#E69F00",
        "#CC79A7"
      ]
    }
  },
  "overrides": {},
  "transforms": {
    "trim": {
      "axis": "both"
    },
    "offset": {
      "distance": 10
    }
  }
}